
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="CubeCart Test Suite" parallel="tests" thread-count="2">
    <test verbose="1" name="Chrome Test">
        <parameter name="browsername" value="chrome"></parameter>
        <classes>
            <class name="test.testngframework.MultiBrowserTest"></class>
        </classes>
    </test>

    <test verbose="1" name="Fire Fox Test">
        <parameter name="browsername" value="fire fox"></parameter>
        <classes>
            <class name="test.testngframework.MultiBrowserTest"></class>
        </classes>
    </test>
</suite>